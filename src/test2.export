template \[%C\] %a[%f|%n; | and ]%F, %G%A: \"%T\"[, %B][, %J][, %?][, %$][, vol.~%V][, no.~%N][, pp.~%P][, %$][, %S][, %I][, %i][, %@][, [[%M %D, |%M~]%Y].\nURL: %U.\n


